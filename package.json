{
	"name": "gutenberg-packages",
	"version": "2.0.0",
	"description": "Monorepo containing all Yard Gutenberg packages",
	"author": {
		"name": "Yard | Digital Agency",
		"email": "info@yard.nl",
		"url": "https://www.yard.nl/"
	},
	"license": "ISC",
	"private": true,
	"scripts": {
		"lint": "wp-scripts lint-js ./packages --fix",
		"lint:scss": "wp-scripts lint-style ./packages",
		"lint:js": "wp-scripts lint-js ./packages",
		"format": "wp-scripts format ./packages --write",
		"auto-changelog": "auto-changelog --tag-pattern .+ --commit-limit false",
		"test": "jest",
		"test:watch": "jest --watch",
		"storybook": "start-storybook -p 6006",
		"packages-update": "wp-scripts packages-update"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:yardinternet/gutenberg-packages.git"
	},
	"bugs": {
		"url": "https://github.com/yardinternet/gutenberg-packages/issues"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.js": [
			"npm run format",
			"npm run lint"
		]
	},
	"workspaces": [
		"packages/*",
		"packages/deprecated/*"
	],
	"devDependencies": {
		"@babel/plugin-transform-class-properties": "^7.22.5",
		"@babel/plugin-transform-runtime": "^7.23.2",
		"@babel/preset-env": "^7.23.2",
		"@emotion/styled": "^11.11.0",
		"@sentry/browser": "^7.77.0",
		"@storybook/addon-a11y": "^7.5.3",
		"@storybook/addon-actions": "^7.5.3",
		"@storybook/addon-knobs": "^7.0.2",
		"@storybook/addon-links": "^7.5.3",
		"@storybook/addon-viewport": "^7.5.3",
		"@storybook/react": "^7.5.3",
		"@testing-library/react": "^10.4.9",
		"@types/jest": "^29.5.7",
		"@wordpress/api-fetch": "^6.52.0",
		"@wordpress/babel-preset-default": "^7.39.0",
		"@wordpress/block-editor": "^12.23.0",
		"@wordpress/blocks": "^12.32.0",
		"@wordpress/components": "^27.3.0",
		"@wordpress/compose": "^6.32.0",
		"@wordpress/data": "^9.25.0",
		"@wordpress/element": "^5.32.0",
		"@wordpress/eslint-plugin": "^17.12.0",
		"@wordpress/hooks": "^3.55.0",
		"@wordpress/i18n": "^4.55.0",
		"@wordpress/icons": "^9.46.0",
		"@wordpress/jest-preset-default": "^11.26.0",
		"@wordpress/notices": "^4.23.0",
		"@wordpress/prettier-config": "^3.12.0",
		"@wordpress/scripts": "^27.6.0",
		"@wordpress/server-side-render": "^4.32.0",
		"@yardinternet/webpack-include-packages": "^1.0.2",
		"auto-changelog": "^2.4.0",
		"classnames": "^2.3.2",
		"emotion": "^11.0.0",
		"husky": "^6.0.0",
		"jest": "*",
		"jest-config": "*",
		"lerna": "^7.4.2",
		"lint-staged": "^10.5.4",
		"lodash": "^4.17.21",
		"postcss": "^8.4.28",
		"postcss-scss": "^4.0.7",
		"prettier": "npm:wp-prettier@^3.0.3",
		"react-select": "^5.8.0",
		"react-test-renderer": "^18.2.0",
		"reactstrap": "^8.10.1",
		"stylelint": "^15.10.3",
		"stylelint-config-idiomatic-order": "^9.0.0",
		"stylelint-config-recommended": "^12.0.0",
		"stylelint-scss": "^5.1.0",
		"typescript": "^4.9.5"
	},
	"dependencies": {
		"@yardinternet/front-render": "*",
		"@yardinternet/gutenberg-block-filters": "*",
		"@yardinternet/gutenberg-cards": "*",
		"@yardinternet/gutenberg-components": "*",
		"@yardinternet/gutenberg-core": "*",
		"@yardinternet/gutenberg-editor-components": "*",
		"@yardinternet/gutenberg-gemeenten": "*",
		"@yardinternet/gutenberg-google-maps": "*",
		"@yardinternet/gutenberg-hooks": "*",
		"@yardinternet/gutenberg-webpack-loaders": "*"
	}
}
