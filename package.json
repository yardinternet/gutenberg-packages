{
	"name": "gutenberg-packages",
	"version": "1.0.0",
	"description": "Monorepo contains all gutenberg packages",
	"author": "Yard Internet",
	"license": "ISC",
	"private": true,
	"scripts": {
		"lint": "wp-scripts lint-js ./packages --fix",
		"format": "wp-scripts format ./packages --write",
		"auto-changelog": "auto-changelog --tag-pattern .+ --commit-limit false",
		"test": "jest",
		"test:watch": "jest --watch",
		"storybook": "start-storybook -p 6006",
		"tsc": "tsc"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:yardinternet/gutenberg-packages.git"
	},
	"bugs": {
		"url": "https://github.com/yardinternet/gutenberg-packages/issues"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged && npm run tsc"
		}
	},
	"lint-staged": {
		"*.js": [
			"npm run format",
			"npm run lint"
		]
	},
	"devDependencies": {
		"@babel/core": "^7.20.5",
		"@babel/plugin-proposal-class-properties": "^7.18.6",
		"@babel/plugin-transform-runtime": "^7.19.6",
		"@babel/preset-env": "^7.20.2",
		"@emotion/styled": "^10.3.0",
		"@sentry/browser": "^5.21.4",
		"@storybook/addon-a11y": "^5.3.22",
		"@storybook/addon-actions": "^5.3.13",
		"@storybook/addon-knobs": "^5.2.8",
		"@storybook/addon-links": "^5.3.13",
		"@storybook/addon-viewport": "^5.2.8",
		"@storybook/react": "^5.3.13",
		"@testing-library/react": "^10.2.1",
		"@types/jest": "^26.0.24",
		"@wordpress/api-fetch": "^3.23.1",
		"@wordpress/babel-preset-default": "^4.17.0",
		"@wordpress/block-editor": "^5.3.3",
		"@wordpress/blocks": "^8.0.3",
		"@wordpress/components": "^13.0.3",
		"@wordpress/compose": "^3.25.3",
		"@wordpress/eslint-plugin": "^13.6.0",
		"@wordpress/icons": "^2.10.3",
		"@wordpress/jest-preset-default": "^6.2.0",
		"@wordpress/prettier-config": "^2.5.0",
		"@wordpress/scripts": "^23.7.2",
		"@yardinternet/webpack-include-packages": "^1.0.2",
		"auto-changelog": "^2.4.0",
		"babel-loader": "^8.3.0",
		"husky": "^4.2.5",
		"jest": "^26.1.0",
		"jest-config": "^26.1.0",
		"lerna": "^3.16.4",
		"lint-staged": "^10.5.4",
		"postcss": "^7.0.39",
		"prettier": "npm:wp-prettier@2.6.2",
		"react-select": "^4.3.1",
		"reactstrap": "^8.10.1",
		"sass": "^1.56.1",
		"sass-loader": "^9.0.2",
		"style-loader": "^1.1.3",
		"stylelint": "^14.14.1",
		"typescript": "^3.9.10",
		"url-loader": "^4.0.0",
		"webpack": "^4.43.0"
	},
	"dependencies": {
		"@yardinternet/front-render": "*",
		"@yardinternet/gutenberg-block-filters": "*",
		"@yardinternet/gutenberg-cards": "*",
		"@yardinternet/gutenberg-core": "*",
		"@yardinternet/gutenberg-editor-components": "*",
		"@yardinternet/gutenberg-gemeenten": "*",
		"@yardinternet/gutenberg-google-maps": "*",
		"@yardinternet/gutenberg-webpack-loaders": "*"
	}
}
