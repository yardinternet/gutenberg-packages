{
	"name": "gutenberg-packages",
	"version": "1.0.0",
	"description": "Monorepo contains all gutenberg packages",
	"author": "Yard Internet",
	"license": "ISC",
	"private": true,
	"scripts": {
		"lint": "wp-scripts lint-js ./packages --fix",
		"format": "wp-scripts format-js ./packages --write",
		"auto-changelog": "auto-changelog --tag-pattern .+ --commit-limit false",
		"test": "jest",
		"test:watch": "jest --watch",
		"storybook": "start-storybook -p 6006",
		"tsc": "tsc"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:yardinternet/gutenberg-packages.git"
	},
	"bugs": {
		"url": "https://github.com/yardinternet/gutenberg-packages/issues"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged && npm run tsc"
		}
	},
	"lint-staged": {
		"*.js": [
			"npm run format",
			"npm run lint"
		]
	},
	"devDependencies": {
		"@babel/core": "^7.8.4",
		"@babel/plugin-transform-runtime": "^7.10.1",
		"@babel/preset-env": "^7.10.4",
		"@emotion/styled": "^10.0.27",
		"@storybook/addon-a11y": "^5.3.14",
		"@storybook/addon-actions": "^5.3.13",
		"@storybook/addon-knobs": "^5.2.8",
		"@storybook/addon-links": "^5.3.13",
		"@storybook/addon-viewport": "^5.2.8",
		"@storybook/react": "^5.3.13",
		"@testing-library/react": "^10.2.1",
		"@types/jest": "^26.0.4",
		"@wordpress/api-fetch": "^3.11.0",
		"@wordpress/babel-preset-default": "^4.17.0",
		"@wordpress/block-editor": "^4.3.1",
		"@wordpress/blocks": "^6.20.0",
		"@wordpress/components": "^10.0.0",
		"@wordpress/eslint-plugin": "^7.1.0",
		"@wordpress/icons": "^2.0.0",
		"@wordpress/jest-preset-default": "^6.2.0",
		"@wordpress/scripts": "^12.0.0",
		"@yardinternet/webpack-include-packages": "^1.0.2",
		"auto-changelog": "^2.0.0",
		"babel-loader": "^8.0.6",
		"css-loader": "^3.4.2",
		"file-loader": "^6.0.0",
		"husky": "^4.2.1",
		"jest": "^26.1.0",
		"jest-config": "^26.1.0",
		"lerna": "^3.16.4",
		"lint-staged": "^10.0.7",
		"postcss": "^7.0.32",
		"prettier": "npm:wp-prettier@2.0.5",
		"sass": "^1.26.10",
		"sass-loader": "^9.0.2",
		"style-loader": "^1.1.3",
		"stylelint": "^13.6.1",
		"typescript": "^3.9.6",
		"url-loader": "^4.0.0",
		"webpack": "^4.43.0"
	},
	"dependencies": {
		"@yardinternet/front-render": "*",
		"@yardinternet/gutenberg-block-filters": "*",
		"@yardinternet/gutenberg-cards": "*",
		"@yardinternet/gutenberg-core": "*",
		"@yardinternet/gutenberg-editor-components": "*",
		"@yardinternet/gutenberg-gemeenten": "*",
		"@yardinternet/gutenberg-google-maps": "*",
		"@yardinternet/gutenberg-webpack-loaders": "*"
	}
}
